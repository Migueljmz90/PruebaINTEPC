<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CompletarCampoMasterSiEsMasterEsTrue</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
   OR(LXD_Cuenta_unica_SKY__c = FALSE,LXD_Cuenta_unica_Novavision__c = FALSE),
   ISBLANK( ParentId )
)</errorConditionFormula>
    <errorDisplayField>ParentId</errorDisplayField>
    <errorMessage>Es obligatorio completar el master.</errorMessage>
</ValidationRule>
